uint GetVertexCount(RegularCellData rcd)
{
		return (rcd.geometryCounts >> 4);
}
	
uint GetTriangleCount(RegularCellData rcd)
{
	return (rcd.geometryCounts & 0x0F);
}

//takes an two 4 bit corner indices and outputs one 4 bit edge index.
uint getEdgeNumber(uint cornerIndices)
{
	uint result = 12;

	/*00000011*/
	if(cornerIndices == 0x01 || cornerIndices == 0x10) result = 0;
	/*00000101*/
	else if(cornerIndices == 0x02 || cornerIndices == 0x20) result = 1;
	/*00001100*/
	else if(cornerIndices == 0x23 || cornerIndices == 0x32) result = 2;
	/*00001010*/
	else if(cornerIndices == 0x13 || cornerIndices == 0x31) result = 3;
	/*00110000*/
	else if(cornerIndices == 0x45 || cornerIndices == 0x54) result = 4;
	/*01010000*/
	else if(cornerIndices == 0x46 || cornerIndices == 0x64) result = 5;
	/*11000000*/
	else if(cornerIndices == 0x67 || cornerIndices == 0x76) result = 6;
	/*10100000*/
	else if(cornerIndices == 0x57 || cornerIndices == 0x75) result = 7;
	/*00010001*/
	else if(cornerIndices == 0x04 || cornerIndices == 0x40) result = 8;
	/*01000100*/
	else if(cornerIndices == 0x26 || cornerIndices == 0x62) result = 9;
	/*10001000*/
	else if(cornerIndices == 0x37 || cornerIndices == 0x73) result = 10;
	/*00100010*/
	else if(cornerIndices == 0x15 || cornerIndices == 0x51) result = 11;
	
	return result;
}

void main()
{ 
	//Corner coordinates, check the diagram to find out what goes where.
	ivec3 corner0;
	
	corner0.x = (x_y_z >> 25) & 31;
	corner0.y = (x_y_z >> 20) & 31;
	corner0.z = (x_y_z >> 15) & 31;
	
	ivec3 corner1;
	ivec3 corner2;
	ivec3 corner3;
	ivec3 corner4;
	ivec3 corner5;
	ivec3 corner6;
	ivec3 corner7;
	
	corner1.x = corner0.x+1;
	corner1.y = corner0.y;
	corner1.z = corner0.z;

	corner2.x = corner0.x;
	corner2.y = corner0.y+1;
	corner2.z = corner0.z;

	corner3.x = corner0.x+1;
	corner3.y = corner0.y+1;
	corner3.z = corner0.z;

	corner4.x = corner0.x;
	corner4.y = corner0.y;
	corner4.z = corner0.z+1;

	corner5.x = corner4.x+1;
	corner5.y = corner4.y;
	corner5.z = corner4.z;

	corner6.x = corner4.x;
	corner6.y = corner4.y+1;
	corner6.z = corner4.z;

	corner7.x = corner4.x+1;
	corner7.y = corner4.y+1;
	corner7.z = corner4.z;

	uint corners = 0;
	
	
	vec4 sample0 = texelFetch(densityVol, corner0, 0);
	vec4 sample1 = texelFetch(densityVol, corner1, 0);
	vec4 sample2 = texelFetch(densityVol, corner2, 0);
	vec4 sample3 = texelFetch(densityVol, corner3, 0);
	vec4 sample4 = texelFetch(densityVol, corner4, 0);
	vec4 sample5 = texelFetch(densityVol, corner5, 0);
	vec4 sample6 = texelFetch(densityVol, corner6, 0);
	vec4 sample7 = texelFetch(densityVol, corner7, 0);
	
	if(sample0.r != 0.0f)
	{
		corners = corners | 1;
	}
	if(sample1.r != 0.0f)
	{
		corners = corners | 2;
	}
	if(sample2.r != 0.0f)
	{
		corners = corners | 4;
	}
	if(sample3.r != 0.0f)
	{
		corners = corners | 8;
	}
	if(sample4.r != 0.0f)
	{
		corners = corners | 16;
	}
	if(sample5.r != 0.0f)
	{
		corners = corners | 32;
	}
	if(sample6.r != 0.0f)
	{
		corners = corners | 64;
	}
	if(sample7.r != 0.0f)
	{
		corners = corners | 128;
	}
	
	uint cellClass = regularCellClass[corners];
	RegularCellData data = regularCellData[cellClass];
	uint vertCount = GetVertexCount(data);
	uint triangleCount = GetTriangleCount(data);
	vVoxel.triangleCount = 0;
	
	
	for(int i = 0; i < triangleCount; i++)
	{
		uint index1 = data.vertexIndex[(i*3)];
		uint index2 = data.vertexIndex[(i*3)+1];
		uint index3 = data.vertexIndex[(i*3)+2];
		
		//These edges are written as a concat of their corners.
		uint edge1 = regularVertexData[12*corners+index1];
		uint edge2 = regularVertexData[12*corners+index2];
		uint edge3 = regularVertexData[12*corners+index3];
		
		//Convert to Edge number, rather than using corners to identify each edge.
		edge1 = getEdgeNumber(edge1 & 0xFF);
		edge2 = getEdgeNumber(edge2 & 0xFF);
		edge3 = getEdgeNumber(edge3 & 0xFF);
		
		//Ensure 4 bits only then shift to posistion.
		edge1 = (edge1 & 0xF) << 8;
		edge2 = (edge2 & 0xF) << 4;
		edge3 = edge3 & 0xF;
		
		
		vVoxel.triangles[i] = x_y_z | edge1 | edge2 | edge3;
		vVoxel.triangleCount += 1;
		
	}
}
